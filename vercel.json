{
  "builds": [
    {
      "src": "apps/api/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/api/api.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/app/assets/$1"
    },
    {
      "src": "/POP\\.Proof_Of_Putt\\.Log\\.576\\.png",
      "dest": "/app/POP.Proof_Of_Putt.Log.576.png"
    },
    {
      "src": "/app$",
      "dest": "/app/index.html"
    },
    {
      "src": "/app/.*",
      "dest": "/app/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
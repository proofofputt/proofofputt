{
  "version": 2,
  "name": "proofofputt",
  "builds": [
    {
      "src": "apps/api/api.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/api/api.py"
    },
    {
      "src": "/app",
      "dest": "/app.html"
    }
  ]
}
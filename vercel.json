{
  "builds": [
    {
      "src": "frontend/webapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/webapp/dist/index.html"
    }
  ]
}